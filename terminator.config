[global_config]
  geometry_hinting = True
  enabled_plugins = LaunchpadBugURLHandler, LaunchpadCodeURLHandler, APTURLHandler, CurrDirOpen
[keybindings]
  split_auto = <Shift><Alt>a
  split_horiz = <Alt>underscore
  split_vert = <Alt>plus
[profiles]
  [[default]]
    background_color = "#1d2021"
    background_darkness = 0.97
    background_type = transparent
    font = Hack Nerd Font 13
    foreground_color = "#ebdbb2"
    scrollback_infinite = True
    palette = "#282828:#cc241d:#98971a:#d79921:#458588:#b16286:#689d6a:#a89984:#928374:#fb4934:#b8bb26:#fabd2f:#83a598:#d3869b:#8ec07c:#ebdbb2"
    use_system_font = False
    title_hide_sizetext = True
    title_transmit_bg_color = "#1e1e1e"
    title_receive_bg_color = "#6b94d6"
    title_inactive_fg_color = "#3d3846"
    title_inactive_bg_color = "#191919"
    title_use_system_font = False
[layouts]
  [[default]]
    [[[window0]]]
      type = Window
      parent = ""
      order = 0
      size = 1200, 800
      position = 200:200
      maximised = False
      fullscreen = False
      title = dg@aether:~
      last_active_term = 08d9e3ab-7974-4653-8e31-811efaba602c
      last_active_window = True
    [[[terminal1]]]
      type = Terminal
      parent = child0
      order = 0
      profile = default
      uuid = 08d9e3ab-7974-4653-8e31-811efaba602c
